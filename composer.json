{
	"name": "lille-bitte/emitter",
	"type": "library",
	"description": "HTTP Response Emitter",
	"license": "BSD-3-Clause",
	"keywords": [
		"emitter",
		"http-emitter",
		"response-emitter",
		"http-response-emitter"
	],
	"config": {
		"sort-packages": true
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0-dev"
		}
	},
	"autoload": {
		"psr-4": {
			"LilleBitte\\Emitter\\": "src/"
		}
	},
	"autoload-dev": {
		"files": [
			"tests/functions.php"
		],
		"psr-4": {
			"LilleBitte\\Emitter\\Tests\\": "tests/"
		}
	},
	"require": {
		"php": "^7.1",
		"psr/http-message": "^1.0"
	},
	"require-dev": {
		"lille-bitte/messenger": "^1.0",
		"phpunit/phpunit": "^7.5"
	}
}
